<% post['terms']['category'].each do |cat| %>
  <% if cat['slug'].include?('front-page') %>
  <% else %>
    <% content_for :title do %>
      <%= post['title'] %>
    <% end %>
  <% end %>
<% end %>

<!--actual page below-->
<div class="row">
  <article class="span-12">
    <main class="legible">
      <%= post['content'] %>
    </main>
  </article>
</div>
